@{
}
@using Intex2.Models
@model Intex2.Models.ViewModels.BurialsViewModel


<h1>Burial Mains</h1>
@foreach (var item in Model.Burialmains)
{
    <ul>
        <li>Burial ID: @item.Id</li>
        <li>Burial Depth: @item.Depth</li>
        <li>Sex: @item.Sex</li>
        <li>Head Direction: @item.Headdirection</li>
        <li>Hair Color: @item.Haircolor</li>
        <li>Age at Death: @item.Ageatdeath</li>
        <li>Textile Color</li>
        <li>Textile Structure</li>
        <li>Textile Function</li>
        <li>Estimate Stature</li>
    </ul>
    <br />
}
